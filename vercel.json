{
  "version": 2,
  "framework": "angular",
  "buildCommand": "npm run build",
  "outputDirectory": "dist/cloudsys-portal-web/browser",

  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "cloudsys-portal.vercel.app"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]

  
}
